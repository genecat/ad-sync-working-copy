{
  "rewrites": [
    {
      "source": "/api/serve-ad/:listingId",
      "destination": "/api/serve-ad/[listingId]"
    },
    {
      "source": "/api/serve-campaign/:campaignId",
      "destination": "/api/serve-campaign/[campaignId]"
    },
    {
      "source": "/api/track-click",
      "destination": "/api/track-click"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/serve-ad/[listingId].js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/serve-campaign/[campaignId].js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/track-click.js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/generate-embed.js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/proxy.js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/record-impression.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS, PATCH, DELETE, PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

